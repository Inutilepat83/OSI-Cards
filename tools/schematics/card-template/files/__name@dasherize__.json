{
  "cardTitle": "<%= strings.classify(name) %> Card",
  "cardSubtitle": "Template generated on <%= date %>",
  "cardType": "<%= cardType %>",
  "description": "A card template for <%= cardType %> type cards",
  "columns": 2,
  "sections": [
    {
      "type": "info",
      "title": "Section 1",
      "fields": [
        {
          "label": "Field 1",
          "value": "Value 1"
        },
        {
          "label": "Field 2",
          "value": "Value 2"
        }
      ]
    },
    {
      "type": "list",
      "title": "Section 2",
      "items": [
        {
          "title": "Item 1",
          "description": "Description 1",
          "value": "Value 1"
        }
      ]
    },
    {
      "type": "analytics",
      "title": "Section 3",
      "fields": [
        {
          "label": "Metric 1",
          "value": 100,
          "format": "number",
          "trend": "up",
          "change": 10
        }
      ]
    }
  ],
  "actions": [
    {
      "label": "Contact",
      "type": "mail",
      "email": {
        "contact": {
          "name": "Support",
          "email": "support@example.com",
          "role": "Support Team"
        },
        "subject": "Inquiry about <%= strings.classify(name) %>",
        "body": "Hello, I would like to inquire about..."
      }
    }
  ]
}
