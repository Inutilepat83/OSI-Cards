{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "projects/osi-cards-lib/src/public-api.ts"
  ],
  "out": "docs/api",
  "exclude": [
    "**/*.spec.ts",
    "**/node_modules/**"
  ],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "readme": "README.md",
  "name": "OSI Cards API Documentation",
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Components",
    "Services",
    "Utilities",
    "Types",
    "Models",
    "*"
  ],
  "gitRevision": "main",
  "plugin": [],
  "theme": "default",
  "hideGenerator": false,
  "searchInComments": true,
  "cleanOutputDir": true
}
