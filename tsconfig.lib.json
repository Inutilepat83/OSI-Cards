{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/lib",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/main.ts",
    "src/polyfills.ts",
    "src/app/app.component.ts",
    "src/app/app.config.ts",
    "src/app/app.routes.ts"
  ]
}








