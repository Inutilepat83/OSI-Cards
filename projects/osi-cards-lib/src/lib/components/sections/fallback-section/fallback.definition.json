{
  "type": "fallback",
  "name": "Fallback Section",
  "description": "Default section renderer for unknown or unsupported section types.",
  "selector": "lib-fallback-section",
  "useCases": ["Unknown types", "Error handling", "Graceful degradation"],
  "bestPractices": [
    "Display section data in readable format",
    "Show section type for debugging",
    "Provide helpful error messages"
  ],
  "rendering": {
    "usesFields": true,
    "usesItems": true,
    "defaultColumns": 1,
    "supportsCollapse": true,
    "supportsEmoji": true
  },
  "completionRules": {
    "minFields": 0,
    "minItems": 0,
    "requireNonEmptyValues": false
  },
  "isInternal": true,
  "examples": {
    "example": {
      "title": "Unknown Section Type",
      "type": "unknown-custom-type",
      "description": "This section type is not recognized and will render using fallback",
      "fields": [
        {
          "label": "Custom Field 1",
          "value": "Some custom data"
        },
        {
          "label": "Custom Field 2",
          "value": "More data to display"
        },
        {
          "label": "Numeric Data",
          "value": 42
        },
        {
          "label": "Status",
          "value": "Active"
        }
      ],
      "items": [
        {
          "title": "Custom Item 1",
          "description": "Description for custom item"
        },
        {
          "title": "Custom Item 2",
          "description": "Another custom item"
        }
      ],
      "preferredColumns": 1,
      "priority": 3
    }
  }
}
