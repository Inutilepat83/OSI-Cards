{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "osi-cards": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/osi-cards",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js", "src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/app/assets",
                "output": "assets/ng-doc/app"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/ui-kit/assets",
                "output": "assets/ng-doc/ui-kit"
              }
            ],
            "styles": [
              "node_modules/@ng-doc/app/styles/global.css",
              "src/styles.scss",
              "src/styles.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src"],
              "sass": {
                "silenceDeprecations": ["import", "mixed-decls"]
              }
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "html2canvas",
              "html2pdf.js",
              "dompurify",
              "dom-to-image-more",
              "jspdf",
              "canvg",
              "leaflet",
              "raf",
              "rgbcolor",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.string.starts-with.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.string.ends-with.js",
              "core-js/modules/es.string.split.js",
              "core-js/modules/es.string.trim.js",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.string.includes.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.regexp.to-string.js"
            ],
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "400kb",
                "maximumError": "600kb"
              },
              {
                "type": "bundle",
                "maximumWarning": "500kb",
                "maximumError": "1mb"
              },
              {
                "type": "bundle",
                "name": "*ai-card*",
                "maximumWarning": "350kb",
                "maximumError": "500kb"
              }
            ],
            "optimization": {
              "fonts": {
                "inline": false
              }
            }
          },
          "configurations": {
            "production": {
              "stylePreprocessorOptions": {
                "sass": {
                  "silenceDeprecations": ["import", "mixed-decls"]
                }
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "400kb",
                  "maximumError": "600kb"
                },
                {
                  "type": "bundle",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "bundle",
                  "name": "*ai-card*",
                  "maximumWarning": "350kb",
                  "maximumError": "500kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": {
                  "inline": false
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "extractLicenses": true,
              "crossOrigin": "anonymous",
              "subresourceIntegrity": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "osi-cards:build",
            "port": 4200,
            "liveReload": true,
            "watch": true,
            "poll": 500
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/app/assets",
                "output": "assets/ng-doc/app"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@ng-doc/ui-kit/assets",
                "output": "assets/ng-doc/ui-kit"
              }
            ],
            "styles": [
              "node_modules/@ng-doc/app/styles/global.css",
              "src/styles.scss",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "sales-assistance-demo": {
      "projectType": "application",
      "root": "sales_assistance_frontend-OSI-cards",
      "sourceRoot": "sales_assistance_frontend-OSI-cards/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/sales-assistance-demo",
            "index": "sales_assistance_frontend-OSI-cards/src/index.html",
            "browser": "sales_assistance_frontend-OSI-cards/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "sales_assistance_frontend-OSI-cards/tsconfig.app.json",
            "styles": [
              "sales_assistance_frontend-OSI-cards/src/styles.scss",
              "dist/osi-cards-lib/styles/_styles-scoped.scss"
            ],
            "scripts": [],
            "stylePreprocessorOptions": {
              "includePaths": ["src"],
              "sass": {
                "silenceDeprecations": ["import", "mixed-decls"]
              }
            },
            "allowedCommonJsDependencies": [
              "html2canvas",
              "html2pdf.js",
              "dompurify",
              "dom-to-image-more",
              "jspdf",
              "canvg",
              "leaflet",
              "raf",
              "rgbcolor",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.string.starts-with.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.string.ends-with.js",
              "core-js/modules/es.string.split.js",
              "core-js/modules/es.string.trim.js",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.string.includes.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.regexp.to-string.js"
            ],
            "budgets": [
              {
                "type": "anyComponentStyle",
                "maximumWarning": "400kb",
                "maximumError": "600kb"
              }
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "sales-assistance-demo:build:production"
            },
            "development": {
              "buildTarget": "sales-assistance-demo:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4401
          }
        }
      }
    },
    "osi-cards-lib": {
      "projectType": "library",
      "root": "projects/osi-cards-lib",
      "sourceRoot": "projects/osi-cards-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/osi-cards-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/osi-cards-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/osi-cards-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/osi-cards-lib/tsconfig.spec.json",
            "karmaConfig": "projects/osi-cards-lib/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": ["@ng-doc/builder", "angular-eslint"]
  }
}
