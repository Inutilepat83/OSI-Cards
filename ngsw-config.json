{
  "$schema": "https://angular.io/schemas/service-worker-config/1.0/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "1.5.3",
    "buildDate": "2025-12-01T00:00:00.000Z",
    "features": {
      "offlineSupport": true,
      "backgroundSync": true,
      "pushNotifications": false
    }
  },
  "assetGroups": [
    {
      "name": "app-shell",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/main*.js",
          "/polyfills*.js",
          "/runtime*.js",
          "/styles*.css"
        ]
      }
    },
    {
      "name": "critical-chunks",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/chunk-home*.js",
          "/chunk-docs-wrapper*.js",
          "/chunk-getting-started*.js",
          "/chunk-installation*.js",
          "/chunk-library-usage*.js",
          "/chunk-best-practices*.js"
        ]
      }
    },
    {
      "name": "feature-chunks",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/chunk-features*.js",
          "/chunk-templates*.js",
          "/chunk-pricing*.js",
          "/chunk-about*.js",
          "/chunk-contact*.js",
          "/chunk-support*.js"
        ]
      }
    },
    {
      "name": "docs-pages",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/chunk-*.js"]
      }
    },
    {
      "name": "assets-critical",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/configs/manifest.json",
          "/assets/i18n/en.json"
        ]
      }
    },
    {
      "name": "assets-lazy",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/favicon.ico"]
      }
    },
    {
      "name": "fonts",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "docs-content",
      "urls": ["/docs/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "3s"
      }
    },
    {
      "name": "api",
      "urls": ["/api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "10s"
      }
    },
    {
      "name": "configs",
      "urls": ["/assets/configs/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "30d",
        "timeout": "5s"
      }
    },
    {
      "name": "card-configs",
      "urls": [
        "/assets/configs/companies/**",
        "/assets/configs/contacts/**",
        "/assets/configs/events/**",
        "/assets/configs/opportunities/**",
        "/assets/configs/products/**",
        "/assets/configs/sko/**",
        "/assets/configs/financials/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 500,
        "maxAge": "7d",
        "timeout": "10s"
      }
    },
    {
      "name": "images",
      "urls": [
        "/assets/**/*.jpg",
        "/assets/**/*.jpeg",
        "/assets/**/*.png",
        "/assets/**/*.webp",
        "/assets/**/*.svg"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 200,
        "maxAge": "30d",
        "timeout": "10s"
      }
    },
    {
      "name": "offline-fallback",
      "urls": ["/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 10,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "user-templates",
      "urls": ["/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "5s"
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/**/*__*/**"]
}
